{
    "name": "vinelab/api-manager",
    "description": "Laravel API Manager Package - beatify and unify your responses with the least effort possible.",
    "keywords": ["api", "laravel", "package", "response"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mahmoud Zalt",
            "email": "mahmoud@vinelab.com"
        },
        {
            "name": "Abed Halawi",
            "email": "abed.halawi@vinelab.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.*",
        "illuminate/config": "5.*",
        "illuminate/http": "5.*",
        "illuminate/routing": "5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*",
        "illuminate/pagination": "5.*"
    },
    "autoload": {
        "classmap": [
            "tests/Vinelab/TestCase.php"
        ],
        "psr-0": {
            "Vinelab\\Api\\": "src/"
        }
    },
    "minimum-stability": "stable"
}
